<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sist.dao.emp-mapper">
  <select id="empListData" resultType="EmpVO">
    SELECT empno,ename,job,hiredate,sal,deptno FROM emp
  </select>
  <select id="empDetailData" resultType="EmpVO" parameterType="int">
    SELECT empno,ename,job,hiredate,sal,deptno FROM emp WHERE empno=#{empno}
  </select>
</mapper>